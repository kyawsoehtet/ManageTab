{
    "manifest_version": 2,
    "name": "Tab Changer",
    "version": "1.0",
    "description": "Click To Change the tab as show",

    "icons": {
        "1": "pics/tab_1.ico",
        "2": "pics/tab_2.png"

      },


    "permissions": [
        "tabs"
    ],

    "browser_action": {
        "browser_style": true,
        "default_icon": "pics/tab_2.png",
        "default_title": "Tab Manager",
        "default_popup": "tabs.html"
    }
}
